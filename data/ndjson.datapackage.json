{
  "name":"ndjson-package",
  "resources":[
     {
        "name":"table2",
        "type":"table",
        "path":"table.ndjson",
        "scheme":"file",
        "format":"ndjson",
        "encoding":"utf-8",
        "mediatype":"text/ndjson",
        "dialect":{
           "csv":{
              "delimiter":"\t",
              "skipInitialSpace":true
           },
           "json":{
              "keyed":true
           }
        },
        "schema":{
           "fields":[
              {
                 "name":"id",
                 "type":"integer"
              },
              {
                 "name":"name",
                 "type":"string"
              }
           ]
        }
     }
  ]
}